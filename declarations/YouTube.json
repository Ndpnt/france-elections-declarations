{
  "name": "YouTube",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://policies.google.com/privacy?hl=fr",
      "select": "div[role=\"article\"]",
      "remove": "img[src$='.svg'], img[src^='data:image/svg+xml']"
    },
    "Terms of Service": {
      "fetch": "https://www.youtube.com/t/terms",
      "select": "div#main-content"
    },
    "Community Guidelines": {
      "fetch": "https://www.youtube.com/intl/ALL_fr/howyoutubeworks/policies/community-guidelines/",
      "select": [".ytr-hero__details", ".ytr-article__content-wrapper"],
      "remove": [".ytr-hero__eyebrow", ".ytr-link > svg"]
    },
    "Community Guidelines - Self-harm": {
      "fetch": "https://support.google.com/youtube/answer/2802245",
      "select": [".article-container"],
      "remove": [".scClientFeedbackInlineFeedbacklinkroot"]
    },
    "Community Guidelines - Hate Speech": {
      "fetch": "https://support.google.com/youtube/answer/2801939",
      "select": [".article-container"],
      "remove": [".scClientFeedbackInlineFeedbacklinkroot"]
    }
  }
}
