{
  "name": "Twitter",
  "documents": {
    "Terms of Service": {
      "fetch": "https://twitter.com/fr/tos",
      "select": [".p22-chapters-container"],
      "remove": [".p05-chapter-navigation", "[style='display: none;']"]
    },
    "Privacy Policy": {
      "fetch": "https://twitter.com/fr/privacy",
      "remove": [
        ".twtr-icon--base",
        ".p05-chapter-navigation",
        "[style='display: none;']",
        ".twtr-overlay__icon",
        ".p21-hidden-content"
      ],
      "select": [
        ".p02-statement-masthead",
        ".p03-scrolling-checklist",
        ".p22-chapters-container",
        ".cr01-section"
      ]
    },
    "Community Guidelines - Self-harm": {
      "fetch": "https://help.twitter.com/fr/rules-and-policies/glorifying-self-harm",
      "select": ["#twtr-main"],
      "remove": [
        ".u11__breadcrumbs",
        ".b24-share-tweet",
        ".b01__headline.twtr-type--headline-md.inherit"
      ]
    },
    "Community Guidelines - Hate Speech": {
      "fetch": "https://help.twitter.com/fr/rules-and-policies/hateful-conduct-policy",
      "select": ["#twtr-main"],
      "remove": [
        ".u11__breadcrumbs",
        ".b24-share-tweet",
        ".b01__headline.twtr-type--headline-md.inherit"
      ]
    },
    "Community Guidelines - Child Sexual Exploitation": {
      "fetch": "https://help.twitter.com/fr/rules-and-policies/sexual-exploitation-policy",
      "select": ["#twtr-main"],
      "remove": [
        ".u11__breadcrumbs",
        ".b24-share-tweet",
        ".b01__headline.twtr-type--headline-md.inherit"
      ]
    }
  }
}
